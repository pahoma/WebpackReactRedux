{
  "name": "webpack-react-redux",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "git@github.com:pahoma/WebpackReactRedux.git",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --progress",
    "build": "NODE_ENV=production webpack --progress  --display-modules",
    "lint": "NODE_ENV='development' ./node_modules/.bin/eslint src",
    "lint:fix": "NODE_ENV='development' ./node_modules/.bin/eslint src --fix",
    "test": "jest",
    "server": "NODE_ENV='development' ./node_modules/nodemon/bin/nodemon.js ./backend/app.js --config local"
  },
  "jest": {
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "^.*[.](jpg|JPG|gif|GIF|png|PNG|less|LESS|css|CSS)$": "EmptyModule"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils",
      "./EmptyModule.js"
    ]
  },
  "author": "Sergey Pakhomov",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.2.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-preset-stage-1": "^6.1.18",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "lodash": "^3.10.1",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.4",
    "redux": "^3.0.4",
    "redux-thunk": "^2.2.0"
  }
}
